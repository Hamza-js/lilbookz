{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["src/*"],
      "@public/*": ["public/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "additional.d.ts",
    "**/*.js",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.mjs",
    ".next/types/**/*.ts"
, "src/app/(hydrogen)/students/current/page.js", "src/app/auth/(sign-in)/sign-in/sign-in-form.tsx", "src/app/shared/students/student-filters/index.jsx", "src/app/auth/(forgot-password)/forgot-password/forgot-password-form.tsx", "src/app/shared/students/student-filters/drawer-view.tsx", "src/app/(hydrogen)/students/email/page.js", "src/app/(hydrogen)/students/email/ClassesFilter.js", "src/app/(hydrogen)/students/cancelled/page.js"  ],
  "exclude": ["node_modules"]
}
